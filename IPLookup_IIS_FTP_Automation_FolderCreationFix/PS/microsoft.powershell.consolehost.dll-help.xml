  			<dev:code>start-transcript</dev:code>
  			<dev:remarks>
				<maml:para>This command starts a transcript in the default file location.</maml:para>
				<maml:para></maml:para>				
				<maml:para></maml:para>
   				<maml:para></maml:para>
				<maml:para></maml:para>
